<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Test Suite" parallel="tests" thread-count="1">
    <listeners>
        <listener class-name="com.test.utils.TestListener"/>
    </listeners>

    <test name="Admin Tests">
        <classes>
            <class name="AdminTest"/>
        </classes>
    </test>

    <test name="Application Tests">
        <classes>
            <class name="CreateEveryApplicationTest"/>
        </classes>
    </test>
</suite>